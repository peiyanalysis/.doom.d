&#23558;&#19978;&#38754;&#30340;&#37197;&#32622;&#25335;&#36125;&#21040;&#20320;&#30340; Emacs &#37197;&#32622;&#20013;&#65292;&#24182;&#32622;&#20110;&#25152;&#26377; org-roam &#30456;&#20851;&#37197;&#32622;&#30340;&#21518;&#38754;&#65292;&#23601;&#21487;&#20197;&#22312;&#20320;&#30340; org-roam &#20013;&#20351;&#29992;&#36825;&#20010;&#27169;&#26495;&#65292;&#21518;&#38754;&#30340;&#31034;&#20363;&#27169;&#26495;&#20063;&#26159;&#19968;&#26679;&#65292;&#20294;&#35201;&#27880;&#24847;&#19981;&#21516;&#27169;&#26495;&#30340; key &#19981;&#35201;&#26377;&#20914;&#31361;&#12290;
