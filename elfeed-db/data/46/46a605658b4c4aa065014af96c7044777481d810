<pre><span>export</span> <span>STANFORD_SEGMENTER_PATH</span>=<span>"$HOME/stanford/segmenter"</span>
<span>export</span> <span>CLASSPATH</span>=<span>"$CLASSPATH:$STANFORD_SEGMENTER_PATH/stanford-segmenter.jar:$STANFORD_SEGMENTER_PATH/slf4j-api.jar"</span>
</pre>
</div></li>
</ol>

