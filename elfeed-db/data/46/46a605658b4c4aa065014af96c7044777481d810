<span>export</span> <span>STANFORD_POSTAGGER_PATH</span>=<span>"$HOME/stanford/postagger"</span>
<span>export</span> <span>CLASSPATH</span>=<span>"$CLASSPATH:$STANFORD_NER_PATH/stanford-ner.jar:$STANFORD_POSTAGGER_PATH/stanford-postagger.jar"</span>
</pre>
</div>

<p>
