<pre><span>CC_RESET</span>=<span>'[00m'</span>

<span>CC_BLACK</span>=<span>'[0;30m'</span>
<span>CC_RED</span>=<span>'[0;31m'</span>
<span>CC_GREEN</span>=<span>'[0;32m'</span>
<span>CC_BROWN</span>=<span>'[0;33m'</span>
<span>CC_BLUE</span>=<span>'[0;34m'</span>
<span>CC_PURPLE</span>=<span>'[0;35m'</span>
<span>CC_CYAN</span>=<span>'[0;36m'</span>

<span>CC_LIGHT_GRAY</span>=<span>'[0;37m'</span>
<span>CC_DARK_GRAY</span>=<span>'[1;30m'</span>
<span>CC_LIGHT_RED</span>=<span>'[1;31m'</span>
<span>CC_LIGHT_GREEN</span>=<span>'[1;32m'</span>
<span>CC_YELLOW</span>=<span>'[1;33m'</span>
<span>CC_LIGHT_BLUE</span>=<span>'[1;34m'</span>
<span>CC_LIGHT_PURPLE</span>=<span>'[1;35m'</span>
<span>CC_LIGHT_CYAN</span>=<span>'[1;36m'</span>
<span>CC_WHITE</span>=<span>'[1;37m'</span>

<span>force_color_prompt</span>=yes
<span>export</span> <span>PS1</span>=<span>'\[\e${CC_WHITE}\][\[\e${CC_RESET}\]\</span>
<span>\[\e${CC_LIGHT_BLUE}\]\w\</span>
<span>\[\e${CC_RESET}\]\[\e${CC_WHITE}\]]\[\e${CC_RESET}\]\n\</span>
