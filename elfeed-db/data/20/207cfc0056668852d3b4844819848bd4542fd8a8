<pre><span>class</span> <span>Point</span>
{
<span>public</span>:
    <span>Point</span>(): _x(0), <span>_y</span>(0) {}
    <span>Point</span>(<span>int</span> <span>x</span>, <span>int</span> <span>y</span>): _x(x), <span>_y</span>(<span>y</span>) {}
    ~<span>Point</span>();

<span>private</span>:
    <span>int</span> <span>_x</span>;
    <span>int</span> <span>_y</span>;
};
</pre>
