<span>10: </span>        <span>if</span> (str[index] != del) {
<span>11: </span>            ++cur;
<span>12: </span>            temp = str[cur];
<span>13: </span>            str[cur] = str[index];
<span>14: </span>            str[index] = str[cur];
<span>15: </span>        }
<span>16: </span>    }
